<div class="container">
<h1 class="mt-3">Edit task</h1>
<div class="form-group mt-3">
<%= form_for(@task) do |f| %>
  <!-- <div class="form-group"> -->
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control mb-3" %>
    <%= f.label :details %>
    <%= f.text_field :details, class: "form-control mb-3"  %>

    <p>Complete?</p><%= f.check_box :completed, class: "form-control mb-3"  %>

    <%= f. submit class: 'btn btn-primary' %>
<% end %>
</div>
</div>
